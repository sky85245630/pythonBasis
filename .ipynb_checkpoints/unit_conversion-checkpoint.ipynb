   "source": [
    "# %f代表loat\n",
    "value = float(input('請輸入長度：'))\n",
    "unit = input('請輸入單位：')\n",
    "\n",
    "if unit == \"in\":\n",
    "    print('%f in = %f cm' % (value , value*2.54))\n",
    "elif unit == \"cm\":\n",
    "    print('%f in = %f cm' % (value , value/2.54))\n",
    "else:\n",
    "    print('請輸入cm或in')"
   ]
